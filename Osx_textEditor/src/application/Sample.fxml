<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.SampleController">
   <children>
      <Pane prefHeight="200.0" prefWidth="200.0">
         <children>
            <Text layoutX="28.0" layoutY="46.0" strokeType="OUTSIDE" strokeWidth="0.0" text="File Name:" />
            <Text layoutX="28.0" layoutY="84.0" strokeType="OUTSIDE" strokeWidth="0.0" text="File Path:" />
            <TextField fx:id="FileName" layoutX="127.0" layoutY="29.0" />
            <TextField fx:id="FilePath" layoutX="127.0" layoutY="67.0" prefHeight="24.0" prefWidth="320.0" />
         </children>
      </Pane>
      <Pane prefHeight="225.0" prefWidth="725.0">
         <children>
            <TextArea fx:id="textField" layoutX="14.0" layoutY="-73.0" prefHeight="434.0" prefWidth="648.0" />
         </children>
      </Pane>
      <Pane prefHeight="200.0" prefWidth="200.0">
         <children>
            <Button fx:id="btnSave" layoutX="612.0" layoutY="162.0" mnemonicParsing="false" onAction="#save" text="Save" />
            <Button fx:id="btnOpen" layoutX="540.0" layoutY="162.0" mnemonicParsing="false" onAction="#open" text="Open" />
            <Button fx:id="btnClear" layoutX="468.0" layoutY="162.0" mnemonicParsing="false" onAction="#clear" text="Clear" />
         </children>
      </Pane>
   </children>
</VBox>
